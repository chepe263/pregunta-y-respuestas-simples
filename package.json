{
  "name": "@chepe263/vue-pregunta-y-respuestas-simples",
  "description": "Componente Vue para mostrar una pregunta y sus opciones de respuesta.",
  "version": "0.1.6",
  "private": true,
  "author": "Capitan Nemo",
  "homepage": "https://github.com/chepe263/vue-pregunta-y-respuestas-simples",
  "repository": {
    "type": "git",
    "url": "https://github.com/chepe263/vue-pregunta-y-respuestas-simples.git"
  },
  "license": "MIT",
  "main": "dist/preguntas.umd.js",
  "unpkg": "dist/preguntas.umd.min.js",
  "browser": {
    "./sfc": "src/preguntas/Pregunta.vue"
  },
  "files": [
    "dist",
    "src/components/preguntas"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name preguntas ./src/components/preguntas/Pregunta.vue",
    "build:npm": "vue-cli-service build --target lib --name preguntas ./src/components/preguntas/Pregunta.vue",
    "docs": "vuese gen && vuese serve",
    "docs.vuese": "vuese gen && vuese serve",
    "docs.vuedoc": "npx vuedoc.md \"./src/components/preguntas/Pregunta.vue\" \"./src/components/preguntas/PreguntaRespuestaOpcion.vue\"  --output ./docs",
    "lint": "vue-cli-service lint",
    "lint-fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vuedoc/md": "^1.6.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-styleguidist": "^3.22.3",
    "vue-template-compiler": "^2.6.10"
  }
}
